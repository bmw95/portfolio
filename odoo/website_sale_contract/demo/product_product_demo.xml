<odoo>

    <record id="product_product_1" model="product.product">
        <field name="name">Demo Contract 1</field>
        <field name="uom_id" ref="product.product_uom_unit" />
        <field name="uom_po_id" ref="product.product_uom_unit" />
        <field name="categ_id" ref="product.product_category_5" />
        <field name="type">service</field>
        <field name="description_sale">Demo 1 contract services.</field>
        <field name="pricelist_id" ref="product.list0" />
        <field name="standard_price">500</field>
        <field name="list_price">1500</field>
        <field name="currency_id" ref="base.USD" />
        <field name="default_code">CON1</field>
        <field name="is_contract" eval="True" />
        <field name="website_published" eval="True" />
        <field name="contract_template_id"
               ref="website_portal_contract.account_analytic_contract_1" />
    </record>

